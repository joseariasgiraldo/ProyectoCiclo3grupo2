@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsAvionModel
@{
    var Avion = Model.Avion;
}
 
<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id: </b> @Avion.id</li>
  <li class="list-group-item"> <b>Marca: </b> @Avion.marca</li>
